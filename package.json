{
  "name": "seed",
  "description": "Boilerplate for a multiplayer VR web app hosted on Gomix",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "start": "node server.js",
    "build": "browserify src/index.js -o public/build.js",
    "dev": "budo src/budo-index.js:public/build.js --port 8000 --live --open --dir public",
    "deploy": "ghpages",
    "ghpages": "ghpages"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/polats/seed.git"
  },
  "devDependencies": {
    "budo": "^7.0.0",
    "ghpages": "0.0.3",
    "aframe": "^0.5.0"
  },
  "dependencies": {
    "express": "^4.14.1",
    "socket.io": "^1.7.2",
    "socket.io-client": "^1.7.2",
    "browserify": "^12.0.1"
  },
  "keywords": [
    "aframe",
    "aframe-example",
    "aframe-boilerplate",
    "aframe-scene",
    "webvr",
    "vr"
  ]
